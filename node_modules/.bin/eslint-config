#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/huang/uniapp/unibest-main/node_modules/.pnpm/@antfu+eslint-config@5.2.1_@unocss+eslint-plugin@66.5.0_eslint@9.34.0_jiti@2.6.1__types_2d9bb3ec209fb239e832caef3c568905/node_modules/@antfu/eslint-config/bin/node_modules:/Users/huang/uniapp/unibest-main/node_modules/.pnpm/@antfu+eslint-config@5.2.1_@unocss+eslint-plugin@66.5.0_eslint@9.34.0_jiti@2.6.1__types_2d9bb3ec209fb239e832caef3c568905/node_modules/@antfu/eslint-config/node_modules:/Users/huang/uniapp/unibest-main/node_modules/.pnpm/@antfu+eslint-config@5.2.1_@unocss+eslint-plugin@66.5.0_eslint@9.34.0_jiti@2.6.1__types_2d9bb3ec209fb239e832caef3c568905/node_modules/@antfu/node_modules:/Users/huang/uniapp/unibest-main/node_modules/.pnpm/@antfu+eslint-config@5.2.1_@unocss+eslint-plugin@66.5.0_eslint@9.34.0_jiti@2.6.1__types_2d9bb3ec209fb239e832caef3c568905/node_modules:/Users/huang/uniapp/unibest-main/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/huang/uniapp/unibest-main/node_modules/.pnpm/@antfu+eslint-config@5.2.1_@unocss+eslint-plugin@66.5.0_eslint@9.34.0_jiti@2.6.1__types_2d9bb3ec209fb239e832caef3c568905/node_modules/@antfu/eslint-config/bin/node_modules:/Users/huang/uniapp/unibest-main/node_modules/.pnpm/@antfu+eslint-config@5.2.1_@unocss+eslint-plugin@66.5.0_eslint@9.34.0_jiti@2.6.1__types_2d9bb3ec209fb239e832caef3c568905/node_modules/@antfu/eslint-config/node_modules:/Users/huang/uniapp/unibest-main/node_modules/.pnpm/@antfu+eslint-config@5.2.1_@unocss+eslint-plugin@66.5.0_eslint@9.34.0_jiti@2.6.1__types_2d9bb3ec209fb239e832caef3c568905/node_modules/@antfu/node_modules:/Users/huang/uniapp/unibest-main/node_modules/.pnpm/@antfu+eslint-config@5.2.1_@unocss+eslint-plugin@66.5.0_eslint@9.34.0_jiti@2.6.1__types_2d9bb3ec209fb239e832caef3c568905/node_modules:/Users/huang/uniapp/unibest-main/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
else
  exec node  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
fi
